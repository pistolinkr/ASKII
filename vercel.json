{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "functions": {
    "wsgi.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  }
}

